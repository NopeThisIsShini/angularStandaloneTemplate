    "rimraf": "6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "wait-on": "7.2.0",
    "three-orbitcontrols-ts": "^0.1.2",
    "@types/three": "^0.179.0",
    "webdriver-manager": "12.1.9"
  },
  "engines": {
    "node": ">= 22.12.0 >= 24.0.0",
